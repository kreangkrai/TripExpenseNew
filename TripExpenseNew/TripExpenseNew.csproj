		<AndroidSigningKeyStore>D:\keystore\te_release_key.keystore</AndroidSigningKeyStore>
	  <AndroidSigningStorePass>P@ssw0rd</AndroidSigningStorePass>
	  <AndroidSigningKeyAlias>te_ctl</AndroidSigningKeyAlias>
